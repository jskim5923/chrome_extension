{
    "manifest_version": 2,
   
    "name": "HelpDesign Macro",
    "description": "This extension get design job what I want",
    "version": "1.8",
   
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },

    "background": {
      "scripts": [
          "background.js"
      ]
    },
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "unlimitedStorage",
      "cookies",
      "<all_urls>",
      "webRequest",
      "webRequestBlocking"
    ]

  }